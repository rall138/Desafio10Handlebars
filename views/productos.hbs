<table class="table table-dark">
    <thead>
        <th>Nombre</th>
        <th>Precio</th>
        <th>Foto</th>
    </thead>
    <tbody>
        {{#each productos}}
            <tr>
                <td>{{this.nombre}}</td>
                <td>${{this.precio}}</td>
                <td><img src="{{this.foto}}" /></td>
            </tr>
        {{/each}}
    </tbody>
</table>

<a href="/">
    <button class="btn btn-link">Cargar nuevo producto</button>
</a>